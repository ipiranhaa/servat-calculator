    "@material-ui/icons": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-typescript": "^1.1.1",
    "jss": "^9.8.7",
    "next": "^11.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jss": "^8.6.1",
    "redux-actions": "^2.6.5",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/next": "^8.0.3",
    "@types/react-dom": "^16.8.4",
